{
  "name": "photodisplay-module",
  "version": "1.0.0",
  "description": "Photo display module for an Airbnb",
  "main": "index.js",
  "scripts": {
    "build": "webpack -d --watch",
    "build-prod": "webpack -p",
    "start": "nodemon server/server.js",
    "start-docker": "nodemon server/server.js",
    "seedDB": "node db/fakeDataGenerator.js && mysql -u root < db/sql-scripts/schema.sql",
    "test": "jest",
    "test:coverage": "npx jest --coverage",
    "big-data-gen": "node data_gen/dataGen.js"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.jsx": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/"
    ],
    "setupFilesAfterEnv": [
      "./node_modules/jest-environment-enzyme/lib/index.js"
    ],
    "testEnvironment": "jest-environment-enzyme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carebnb/photodisplay-module.git"
  },
  "keywords": [],
  "author": "Scott Torres",
  "contributors": [
    "Daniel Kim"
  ],
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "bugs": {
    "url": "https://github.com/carebnb/photodisplay-module/issues"
  },
  "homepage": "https://github.com/carebnb/photodisplay-module#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.2",
    "lint-staged": "^8.1.7",
    "pre-commit": "^1.2.2",
    "puppeteer": "^1.16.0",
    "react-test-renderer": "^16.8.6",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "mysql": "^2.17.1",
    "nodemon": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0"
  }
}
